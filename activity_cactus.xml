<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF"
    tools:context=".CactusActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <GridLayout
            android:layout_width="372dp"
            android:layout_height="2215dp"
            android:layout_gravity="center_horizontal"
            android:layout_margin="5dp"
            android:background="@android:color/holo_green_light"
            android:columnCount="2"
            android:columnOrderPreserved="false"
            android:rowCount="5">

            <ImageButton
                android:id="@+id/Burrosbutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="0"
                android:layout_column="0"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__50_" />

            <ImageButton
                android:id="@+id/Brainbutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="0"
                android:layout_column="1"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__51_" />

            <ImageButton
                android:id="@+id/Brazilianbutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="1"
                android:layout_column="0"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__52_" />

            <ImageButton
                android:id="@+id/Bunnybutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="1"
                android:layout_column="1"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__53_" />

            <ImageButton
                android:id="@+id/buttonbutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="2"
                android:layout_column="0"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                android:tint="#00FFFFFF"
                app:srcCompat="@drawable/download__54_" />

            <ImageButton
                android:id="@+id/Strawberrybutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="2"
                android:layout_column="1"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__55_" />

            <ImageButton
                android:id="@+id/Catbutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="3"
                android:layout_column="1"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__56_" />

            <ImageButton
                android:id="@+id/Carminebutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="3"
                android:layout_column="0"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__57_" />

            <ImageButton
                android:id="@+id/Dwarfchinbutton"
                android:layout_width="179dp"
                android:layout_height="182dp"
                android:layout_row="4"
                android:layout_column="0"
                android:adjustViewBounds="true"
                android:background="#00FFFFFF"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/download__58_" />
        </GridLayout>

    </LinearLayout>
</ScrollView>
